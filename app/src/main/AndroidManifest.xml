<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fisei.athanasiaapp">
    <!-- Permisos necesarios para la App. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AthanasiaApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".OrderDetailsActivity_SSNT"
            android:exported="false" />
        <activity
            android:name=".SingUpActivity_SSNT"
            android:exported="false" />
        <activity
            android:name=".AthanasiaActivity_SSNT"
            android:exported="false"
            android:label="@string/title_activity_athanasia"
            android:theme="@style/Theme.AthanasiaApp.NoActionBar" />
        <activity
            android:name=".LoginActivity_SSNT"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity_SSNT"
            android:exported="true" />
    </application>

</manifest>